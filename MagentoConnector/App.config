<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

    <appSettings>
      <add key="mageUrl" value="{site_url}" />
      <add key="username" value="{username}" />
      <add key="password" value="{password}" />
      <add key="conversionRate" value="{currency exchange value -- fixed to 3.75}" />
    </appSettings>
</configuration>